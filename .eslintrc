{
  "extends": [
    "next",
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "prefer-const": "error",
    "newline-before-return": "error",
    "@typescript-eslint/consistent-type-imports": "error",
    "object-shorthand": "error"
  }
}
